:: Maybe a = Nothing | Just a
:: Data a = {name :: String, description :: String, value :: a}

id :: a -> a
id a = a

head :: [a] -> Maybe a
head [h:t] = Just h
head _ = Nothing

tail :: [a] -> Maybe [a]
tail [] = Nothing
tail [h : t] = Just t


map :: [a] (a -> b) -> [b]
map [] _ = []
map [x:xs] f = [f x : map xs f]

int :: Maybe (Data Int) -> Int
int Nothing = 0
int (Just {Data|value}) = value

main :: [Data Int] -> Int
main d
# data = head d
= int data

